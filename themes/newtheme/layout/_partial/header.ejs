<header id="header">
  <div id="banner">
    <h1 id="logo-wrap">
      <a id="logo" href="<%= config.root %>"><%= config.title %></a>
    </h1>
  </div>
  <nav id="main-nav">
    <div class="outer">
      <div id="main-nav-inner" class="inner">
        <a id="main-nav-menu-btn">&#xF0C9;</a>
        <a id="main-nav-search-btn">&#xF002;</a>
      </div>
    </div>
    <ul id="main-nav-list"><!--
      <% for (var i in theme.menu){ %>
     --><li class="main-nav-item">
          <a class="main-nav-link" href="<%= theme.menu[i] %>"><%= i %></a>
        </li><!--
      <% } %>
 --></ul>
    <div id="search-form-wrap">
      <div class="outer">
        <div id="search-form-inner" class="inner">
          <%- search_form() %>
          <a id="search-form-close">&#xF00D;</a>
        </div>
      </div>
    </div>
  </nav>
</header>